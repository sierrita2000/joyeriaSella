{
    "version" : 2,
    "functions": {
        "index.js": {
            "runtime": "@vercel/node",
            "routes": [
                { "src" : "/(.*)", "dest": "index.js" }
            ]
        }
    },
    "rewrites": [
        { "src" : "/(.*)", "dest": "index.js" }
    ],
    "static": {
        "public": "public"
    }
}