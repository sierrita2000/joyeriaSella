{
    "version" : 2,
    "functions": {
        "index.js": {
            "runtime": "@vercel/node",
            "routes": [
                { "src" : "/(.*)", "dest": "index.js" }
            ]
        }
    },
    "rewrites": [
        { "src" : "/public/(.*)", "dest": "/public" },
        { "src" : "/(.*)", "dest": "index.js" }
    ]
}