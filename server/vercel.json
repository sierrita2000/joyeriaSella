{
    "version" : 2,
    "builds"  : [ 
        { "src" : "*.png"  , "use" : "@vercel/static" },
        { "src" : "index.js"  , "use" : "@vercel/node" }
    ],
    "routes"  : [ 
        { "src" : "/public/(.*)"        , "dest": "/public/$1" },
        { "src" : "/(.*)"               , "dest": "index.js" }  
    ]
}